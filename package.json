{
  "private": true,
  "name": "sanity-gatsby-blog",
  "version": "1.0.17",
  "scripts": {
    "build": "lerna run build --parallel",
    "dev": "lerna run dev --parallel",
    "format": "lerna run format",
    "build-studio": "lerna bootstrap && cd studio && npm run build",
    "build-web": "lerna bootstrap && (cd studio && SANITY_AUTH_TOKEN=$SANITY_DEPLOY_STUDIO_TOKEN npm run graphql-deploy) && (cd web && npm run build)",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-test": "npm run build-web && netlify dev"
  },
  "devDependencies": {
    "@sanity/cli": "^2.8.0",
    "lerna": "^3.22.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@react-icons/all-files": "^4.1.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^2.8.0",
    "@sanity/image-url": "^0.140.22",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.12.0",
    "fs": "0.0.1-security",
    "gatsby-plugin-fontawesome-css": "^1.1.0",
    "gatsby-plugin-styled-components": "^4.4.0",
    "gatsby-source-filesystem": "^3.8.0",
    "gatsby-source-google-reviews": "0.0.7",
    "google-maps-reviews": "^0.1.1",
    "hls.js": "^1.0.0-alpha.5",
    "imagemin": "^8.0.0",
    "jquery": "^3.6.0",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.2.1",
    "react-icons": "^4.2.0",
    "react-modal": "^3.13.1",
    "react-native-modal": "^11.10.0",
    "react-native-web": "^0.16.1",
    "react-slick": "^0.28.1",
    "sass": "^1.32.12",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.2.3",
    "uniqid": "^5.3.0"
  }
}
